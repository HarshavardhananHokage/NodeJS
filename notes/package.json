{
  "name": "notes",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "nodemon": "nodemon ./bin/www",
    "start-fs": "cross-env DEBUG=notes:* NOTES_MODEL=fs node --experimental-modules ./bin/www.mjs",
    "start-level": "cross-env DEBUG=notes:* NOTES_MODEL=level node --experimental-modules ./bin/www.mjs",
    "start-sqlite3": "cross-env DEBUG=notes:* NOTES_MODEL=sqlite3 node --experimental-modules ./bin/www.mjs",
    "start-sequelize": "cross-env DEBUG=notes:* SEQUELIZE_CONNECT=models/sequelize-sqlite.yaml NOTES_MODEL=sequelize node --experimental-modules ./bin/www.mjs",
    "sqlite3-setup": "node --experimental-modules ./models/createnotesdb.mjs"
  },
  "dependencies": {
    "bootstrap": "^4.2.1",
    "cookie-parser": "~1.4.3",
    "cross-env": "^5.2.0",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "feather-icons": "^4.10.0",
    "fs-extra": "^7.0.1",
    "hbs": "~4.0.1",
    "http-errors": "~1.6.2",
    "jquery": "^3.3.1",
    "js-yaml": "^3.12.1",
    "level": "^4.0.0",
    "morgan": "~1.9.0",
    "popper.js": "^1.14.6",
    "rotating-file-stream": "^1.3.10",
    "sequelize": "^4.42.0",
    "sqlite3": "^4.0.4"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  }
}
